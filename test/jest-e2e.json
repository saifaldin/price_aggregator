{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", { "tsconfig": "tsconfig.spec.json" }]
  },
  "moduleNameMapper": {
    "^\\.\\./(.+)\\.js$": "../$1",
    "^\\./(.+)\\.js$": "./$1"
  }
}
